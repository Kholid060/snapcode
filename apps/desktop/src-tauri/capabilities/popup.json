{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "popup-window",
  "description": "Capability for the popup window",
  "windows": ["popup"],
  "permissions": [
    "core:default",
    "log:default",
    "log:allow-log",
    "core:window:allow-hide",
    "core:window:allow-close",
    "core:window:allow-start-dragging",
    "store:default"
  ]
}
